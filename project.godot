config_version=5

[application]

config/name="Space Wizards"
run/main_scene="res://Level/Level0.tscn"
config/features=PackedStringArray("4.1", "Forward Plus")
run/max_fps=60
config/icon="res://icon.svg"

[autoload]

InputBuffer="*res://Scripts/input_buffer.gd"
EnergyPointContainer="*res://Scenes/energy_point_container.tscn"
globals="*res://globals.gd"
